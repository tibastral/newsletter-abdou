<h1>Listing newsletters</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Artist name</th>
    <th>Artist caption</th>
    <th>Party name</th>
    <th>Party cost</th>
    <th>Party adress</th>
    <th>Party description</th>
  </tr>

<% for newsletter in @newsletters %>
  <tr>
    <td><%=h newsletter.name %></td>
    <td><%=h newsletter.artist_name %></td>
    <td><%=h newsletter.artist_caption %></td>
    <td><%=h newsletter.party_name %></td>
    <td><%=h newsletter.party_cost %></td>
    <td><%=h newsletter.party_adress %></td>
    <td><%=h newsletter.party_description %></td>
    <td><%= link_to 'Show', newsletter %></td>
    <td><%= link_to 'Edit', edit_newsletter_path(newsletter) %></td>
    <td><%= link_to 'Destroy', newsletter, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New newsletter', new_newsletter_path %>
